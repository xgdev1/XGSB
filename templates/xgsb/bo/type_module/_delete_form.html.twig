<form method="post" action="{{ path('xgsb_bo_type_module_delete', {'id': type_module.id}) }}" class="d-flex"
      onsubmit="return confirm('Etes vous sur de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ type_module.id) }}">
    <button class="btn btn-danger align-self-center">
        <span class="icon-delete-outline"></span> Supprimer
    </button>
</form>
